
@model AboutIntro
@{
    ViewData["Title"] = "Update";
}
<div class="content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-6 h3 mb-4 text-gray-800">
                <h4>About Update</h4>
            </div>
            <div class="col-lg-12 mx-auto">
                <form method="post" enctype="multipart/form-data">

                    <div class="form-group">
                        <label asp-for="Feature"></label>
                        <textarea class="form-control" asp-for="Feature" placeholder="Enter Feature"></textarea>
                        <span class="text-danger" asp-validation-for="Feature"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="FeatureAZ"></label>
                        <input class="form-control" asp-for="FeatureAZ" placeholder="Enter Description">
                        <span class="text-danger" asp-validation-for="FeatureAZ"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="FeatureRU"></label>
                        <input class="form-control" asp-for="FeatureRU" placeholder="Enter Description">
                        <span class="text-danger" asp-validation-for="FeatureRU"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Description"></label>
                        <textarea class="form-control" asp-for="Description" placeholder="Enter Description"></textarea>
                        <span class="text-danger" asp-validation-for="Description"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="DescriptionAZ"></label>
                        <input class="form-control" asp-for="DescriptionAZ" placeholder="Enter Description">
                        <span class="text-danger" asp-validation-for="DescriptionAZ"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="DescriptionRU"></label>
                        <input class="form-control" asp-for="DescriptionRU" placeholder="Enter Description">
                        <span class="text-danger" asp-validation-for="DescriptionRU"></span>
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>




