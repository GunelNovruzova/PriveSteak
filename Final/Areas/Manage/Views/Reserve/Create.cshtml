
@model Reserve
@{
    ViewData["Title"] = "Create";
}
<div class="content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-6 h3 mb-4 text-gray-800">
                <h1>Reserve Create</h1>
            </div>

            <div class="col-lg-12 mx-auto">
                <form method="post" enctype="multipart/form-data">
                    <div class="form-group">
                        <label asp-for="Number"></label>
                        <input class="form-control" asp-for="Number" placeholder="Enter Price">
                        <span class="text-danger" asp-validation-for="Number"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="ImageFile"></label>
                        <input class="form-control" accept="image/jpeg" asp-for="ImageFile" placeholder="Enter Image">
                        <span class="text-danger" asp-validation-for="ImageFile"></span>
                    </div>
                    @*<div class="form-group">
                        <label asp-for="ImagePosition"></label>
                        <input class="form-control" asp-for="ImagePosition" placeholder="Enter Price">
                        <span class="text-danger" asp-validation-for="ImagePosition"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="ImageWidth"></label>
                        <input class="form-control" accept="image/jpeg" asp-for="ImageWidth" placeholder="Enter Image">
                        <span class="text-danger" asp-validation-for="ImageWidth"></span>
                    </div>*@
                    @*<div class="form-group">
                        <label asp-for="ImageHeigth"></label>
                        <input class="form-control" accept="image/jpeg" asp-for="ImageHeigth" placeholder="Enter Image">
                        <span class="text-danger" asp-validation-for="ImageHeigth"></span>
                    </div>*@
                    <div class="form-group">
                        <label asp-for="TableCategoryId"></label>
                        <input class="form-control" asp-for="TableCategoryId" placeholder="Enter Price">
                        <span class="text-danger" asp-validation-for="TableCategoryId"></span>
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>
@section Script
{
    <partial name="_ValidationFormScriptPartial"/>

}
